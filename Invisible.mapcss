meta
{
    title: "Invisible";
    description: "Renders all default features in as low-key manner as possible.";
    version: "0.1";
    author: "James Ross";
}

node {
    symbol-size: 2;
    symbol-shape: square;
    symbol-stroke-color: yellow;
    symbol-fill-color: yellow;
}

way {
    width: 1;
    color: #404040;
    fill-color: transparent;
}
